{
    "name": "rinvex/university",
    "description": "Rinvex University is a simple and lightweight package for retrieving university details with flexibility. A whole bunch of data including name, country, state, email, website, telephone, address, and much more attributes for the 17k+ known universities worldwide at your fingertips.",
    "keywords": [
        "rinvex",
        "scripts",
        "university",
        "lightweight",
        "localization",
        "flexible",
        "locale",
        "family",
        "simple"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/university/issues",
        "source": "https://github.com/rinvex/university",
        "docs": "https://github.com/rinvex/university/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/university/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Rinvex\\University\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\University\\Test\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
