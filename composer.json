{
    "name": "rinvex/universities",
    "description": "Rinvex Universities is a simple and lightweight package for retrieving university details with flexibility. A whole bunch of data including name, country, state, email, website, telephone, address, and much more attributes for the 17k+ known universities worldwide at your fingertips.",
    "type": "library",
    "keywords": [
        "rinvex",
        "degree",
        "collage",
        "division",
        "department",
        "lightweight",
        "university",
        "instituion",
        "education",
        "research",
        "language",
        "flexible",
        "simple"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/universities/issues",
        "source": "https://github.com/rinvex/universities",
        "docs": "https://github.com/rinvex/universities/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous PHP Community",
            "homepage": "https://github.com/rinvex/universities/contributors"
        }
    ],
    "require": {
        "php": "^8.1.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.32.0",
        "phpunit/phpunit": "^9.5.0 || ^10.0.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Rinvex\\University\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\University\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
